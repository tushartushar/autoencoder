@Override public byte[] getRow(int y,byte[] row){
  if (y < 0 || y >= getHeight()) {
    throw new IllegalArgumentException("Requested row is outside the image: " + y);
  }
  int width=getWidth();
  if (row == null || row.length < width) {
    row=new byte[width];
  }
  image.getRaster().getDataElements(left,top + y,width,1,row);
  return row;
}
